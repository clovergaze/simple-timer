{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "ordered-imports": false,
    "no-namespace": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-unused-expression": false
  }
}