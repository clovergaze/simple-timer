{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "no-namespace": false,
    "no-unused-expression": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "double"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
